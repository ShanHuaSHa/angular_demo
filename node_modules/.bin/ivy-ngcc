#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_@angular_compiler-cli@7.0.4@@angular/compiler-cli/src/ngcc/main-ngcc.js" "$@"
  ret=$?
else 
  node  "$basedir/../_@angular_compiler-cli@7.0.4@@angular/compiler-cli/src/ngcc/main-ngcc.js" "$@"
  ret=$?
fi
exit $ret
